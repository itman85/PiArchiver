<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>PiArchiverCloud</servlet-name>
		<servlet-class>com.gae.piarchivercloud.PiArchiverCloudServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.gae.piarchivercloud.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LinksServlet</servlet-name>
		<servlet-class>com.gae.piarchivercloud.LinksServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LinkExtensionServlet</servlet-name>
		<servlet-class>com.gae.piarchivercloud.LinkExtensionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CategoryServlet</servlet-name>
		<servlet-class>com.gae.piarchivercloud.CategoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PiArchiverCloud</servlet-name>
		<url-pattern>/piarchivercloud</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinksServlet</servlet-name>
		<url-pattern>/addlink</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinksServlet</servlet-name>
		<url-pattern>/getlinkshistory</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinksServlet</servlet-name>
		<url-pattern>/updatelink</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CategoryServlet</servlet-name>
		<url-pattern>/getcategories</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CategoryServlet</servlet-name>
		<url-pattern>/submitcategories</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinkExtensionServlet</servlet-name>
		<url-pattern>/submitlinkextension</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinkExtensionServlet</servlet-name>
		<url-pattern>/getlinksextension</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
