{
  "name": "PiArchiver",
  "version": "1.0",
  "description": "Bookmark links",
  "background": { "scripts": ["js/jquery.min.js","background.js"] },
  "page_action" :
  {
    "default_icon" : "img/icon-20.png",
    "default_title" : "PiArchiver Extension",
	"default_popup": "html/popup.html"
  },
  "permissions" : [
    "tabs",
      "http://localhost:8888/getcategories"
  ],
  "icons" : {
    "48" : "img/icon-48.png",
    "128" : "img/icon-128.png"
  },
  "manifest_version": 2
}
